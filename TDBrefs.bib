@Article{Leeuwen2011,
  Title                    = {Prognostic Value of Major Extracranial Injury in Traumatic Brain Injury: An Individual Patient Data Meta-analysis in 39,274 Patients},
  Author                   = {van Leeuwen, N. and Lingsma, H. F. and Perel, P. and Lecky, F. and Roozenbeek, B. and Lu, J. and Shakur, H. and Weir, J. and Steyerberg, E. W. and Maas, A. I.},
  Journal                  = {Neurosurgery},
