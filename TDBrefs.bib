@Article{Schindler2011,
  Title                    = {Protecting fragile skin: nursing interventions to decrease development of pressure ulcers in pediatric intensive care},
  Author                   = {Schindler, C. A. and Mikhailov, T. A. and Kuhn, E. M. and Christopher, J. and Conway, P. and Ridling, D. and Scott, A. M. and Simpson, V. S.},
