@Article{Bautista2001,
  Title                    = {Is C-reactive protein an independent risk factor for essential hypertension?},
  Author                   = {Bautista, LE and LÃ³pez-Jaramillo, P and Vera, LM and Casas, JP and Otero, AP and Guaracao, AI},
  Journal                  = {J Hypertens},
