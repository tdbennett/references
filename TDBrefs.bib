@Article{Simma1998,
  Title                    = {A prospective, randomized, and controlled study of fluid management in children with severe head injury: lactated Ringer's solution versus hypertonic saline},
  Author                   = {Simma, B. and Burger, R. and Falk, M. and Sacher, P. and Fanconi, S.},
  Journal                  = {Crit Care Med},
  Year                     = {1998},

  Month                    = {Jul},
  Note                     = {Simma, B
