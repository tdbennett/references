@Article{Angus2003,
  Title                    = {Cost-effectiveness of inhaled nitric oxide in the treatment of neonatal respiratory failure in the United States},
  Author                   = {Angus, D. C. and Clermont, G. and Watson, R. S. and Linde-Zwirble, W. T. and Clark, R. H. and Roberts, M. S.},
  Journal                  = {Pediatrics},
