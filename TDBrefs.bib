@Article{Tepas2009a,
  Title                    = {The effect of delay in rehabilitation on outcome of severe traumatic brain injury},
  Author                   = {Tepas, J. J., 3rd and Leaphart, C. L. and Pieper, P. and Beaulieu, C. L. and Spierre, L. R. and Tuten, J. D. and Celso, B. G.},
  Journal                  = {J Pediatr Surg},
  Year                     = {2009},

  Month                    = {Feb},
  Note                     = {Tepas, Joseph J 3rd
Leaphart, Cynthia L
