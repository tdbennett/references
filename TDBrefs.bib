@Article{Typpo2010,
  Title                    = {Children with chronic illness return to their baseline functional status after organ dysfunction on the first day of admission in the pediatric intensive care unit},
  Author                   = {Typpo, K. V. and Petersen, N. J. and Petersen, L. A. and Mariscalco, M. M.},
