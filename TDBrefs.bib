@Article{Armano2005,
  Title                    = {Determinants of red blood cell transfusions in a pediatric critical care unit: a prospective, descriptive epidemiological study.},
  Author                   = {Armano, R and Gauvin, F and Ducruet, T and Lacroix, J},
  Journal                  = {Crit Care Med},
