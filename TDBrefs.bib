@Article{Kent2008,
  Title                    = {Competing risk and heterogeneity of treatment effect in clinical trials},
  Author                   = {Kent, D. M. and Alsheikh-Ali, A. and Hayward, R. A.},
  Journal                  = {Trials},
  Year                     = {2008},
  Note                     = {Kent, David M
Alsheikh-Ali, Alawi
