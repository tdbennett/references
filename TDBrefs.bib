@Article{Ravelli2005,
  Title                    = {Preliminary diagnostic guidelines for macrophage activation syndrome complicating systemic juvenile idiopathic arthritis.},
  Author                   = {Ravelli, A and Magni-Manzoni, S and Pistorio, A and Besana, C and Foti, T and Ruperto, N and Viola, S and Martini, A},
  Journal                  = {J Pediatr},
  Year                     = {2005},

