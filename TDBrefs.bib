@Article{Ong1996,
  Title                    = {Early post-traumatic seizures in children: clinical and radiological aspects of injury.},
  Author                   = {Ong, L. C. and Dhillon, M. K. and Selladurai, B. M. and Maimunah, A. and Lye, M. S.},
  Journal                  = {J Paediatr Child Health},
  Pmid                     = {9156530},
