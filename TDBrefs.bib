  Author                   = {Ramsey, BW and Pepe, MS and Quan, JM and Otto, KL and Montgomery, AB and Williams-Warren, J and Vasiljev-K, M and Borowitz, D and Bowman, CM and Marshall, BC and Marshall, S and Smith, AL},
  Journal                  = {N Engl J Med},
  Year                     = {1999},

