@Article{Bloom2001,
  Title                    = {Lifetime and novel psychiatric disorders after pediatric traumatic brain injury.},
  Author                   = {Bloom, D. R. and Levin, H. S. and Ewing-Cobbs, L. and Saunders, A. E. and Song, J. and Fletcher, J. M. and Kowatch, R. A.},
