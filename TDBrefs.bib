@Article{Dreyer2010,
  Title                    = {GRACE principles: recognizing high-quality observational studies of comparative effectiveness},
  Author                   = {Dreyer, N. A. and Schneeweiss, S. and McNeil, B. J. and Berger, M. L. and Walker, A. M. and Ollendorf, D. A. and Gliklich, R. E.},
