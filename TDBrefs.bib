@Article{Hammond2015a,
  Title                    = {Readmission to an Acute Care Hospital During Inpatient Rehabilitation for Traumatic Brain Injury.},
  Author                   = {Hammond, Flora M. and Horn, Susan D. and Smout, Randall J. and Beaulieu, Cynthia L. and Barrett, Ryan S. and Ryser, David K. and Sommerfeld, Teri},
  Journal                  = {Arch Phys Med Rehabil},
  Pmid                     = {26212405},
  Year                     = {2015},

