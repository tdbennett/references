}

@Article{Bonnet1999,
  Title                    = {Detection of transposition of the great arteries in fetuses reduces neonatal morbidity and mortality.},
  Author                   = {Bonnet, D and Coltri, A and Butera, G and Fermont, L and Le Bidois, J and Kachaner, J and Sidi, D},
  Journal                  = {Circulation},
