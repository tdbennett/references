  Author                   = {Lacroix, J and HÃ©bert, PC and Hutchison, JS and Hume, HA and Tucci, M and Ducruet, T and Gauvin, F and Collet, JP and Toledano, BJ and Robillard, P and Joffe, A and Biarent, D and Meert, K and Peters, MJ},
  Journal                  = {N Engl J Med},
