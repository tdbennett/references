@Article{Czaja2011,
  Title                    = {Performance of the Pediatric Index of Mortality 2 for pediatric cardiac surgery patients},
  Author                   = {Czaja, A. S. and Scanlon, M. C. and Kuhn, E. M. and Jeffries, H. E.},
  Journal                  = {Pediatric Critical Care Medicine},
