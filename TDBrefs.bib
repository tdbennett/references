@Article{Allan2010,
  Title                    = {Simulation-based training delivered directly to the pediatric cardiac intensive care unit engenders preparedness, comfort, and decreased anxiety among multidisciplinary resuscitation teams},
  Author                   = {Allan, C. K. and Thiagarajan, R. R. and Beke, D. and Imprescia, A. and Kappus, L. J. and Garden, A. and Hayes, G. and Laussen, P. C. and Bacha, E. and Weinstock, P. H.},
  Journal                  = {J Thorac Cardiovasc Surg},
  Year                     = {2010},

  Month                    = {Sep},
  Note                     = {Allan, Catherine K
Thiagarajan, Ravi R
Beke, Dorothy
Imprescia, Annette
Kappus, Liana J
Garden, Alexander
Hayes, Gavin
Laussen, Peter C
