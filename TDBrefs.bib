  Institution              = {Department of Pediatrics, University of Texas-Houston Health Sciences Center, USA.},
  Keywords                 = {Attention Deficit Disorder with Hyperactivity, diagnosis/epidemiology/etiology; Brain Injuries, psychology; Child; Depressive Disorder, Major, diagnosis/epidemiology/etiology; Female; Humans; Male; Prevalence; Psychiatric Status Rating Scales; Time Factors},
