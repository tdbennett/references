@Article{DegliAtti2013,
  Title                    = {Performance of the pediatric index of mortality 2 (PIM-2) in cardiac and mixed intensive care units in a tertiary children's referral hospital in Italy},
  Author                   = {Degli Atti, M. L. and Cuttini, M. and Rava, L. and Rinaldi, S. and Brusco, C. and Cogo, P. and Pirozzi, N. and Picardo, S. and Schiavi, F. and Raponi, M.},
  Journal                  = {BMC Pediatr},
