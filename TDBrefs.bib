@Article{Crouchman2001,
  Title                    = {A practical outcome scale for paediatric head injury.},
  Author                   = {Crouchman, M. and Rossiter, L. and Colaco, T. and Forsyth, R.},
  Journal                  = {Arch Dis Child},
  Pmid                     = {11159284},
  Year                     = {2001},

  Month                    = {Feb},
  Number                   = {2},
  Pages                    = {120--124},
  Volume                   = {84},

  Abstract                 = {Traumatic brain injury (TBI) is the commonest cause of acquired disability in childhood. A major obstacle to the evaluation of acute and rehabilitative therapies after TBI is the lack of simple descriptors of outcome. We developed the King's Outcome Scale for Childhood Head Injury (KOSCHI), as a specific paediatric adaptation of the original adult Glasgow Outcome Scale (GOS). The KOSCHI expands the five category GOS to provide increased sensitivity at the milder end of the disability range. The GOS category of "persistent vegetative state" was replaced by "vegetative". "Good recovery" was allocated two categories, in acknowledgement of the long term importance of relatively minor sequelae in a developing child. The scale was quick and easy to use. Inter-rater reliability studies show that even with such an apparently simple scale, some training may be required. The KOSCHI provides a practical scale for paediatric head injury which will enable clinicians to describe rate and extent of recovery, and evaluate the effects of service and research interventions.},
  Institution              = {Paediatric Neurosciences, King's College Hospital, Denmark Hill, London SE5 9RS, UK. marion.crouchman@kcl.ac.uk},
  Keywords                 = {Adolescent; Child; Child, Preschool; Craniocerebral Trauma, rehabilitation; Humans; Observer Variation; Outcome Assessment (Health Care); Regression Analysis; Sensitivity and Specificity; Severity of Illness Index},
  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {bennette},
  Timestamp                = {2016.02.23}
}

