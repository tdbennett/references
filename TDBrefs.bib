@Article{Werdan2007,
  Title                    = {Score-based immunoglobulin G therapy of patients with sepsis: the SBITS study.},
  Author                   = {Werdan, K and Pilz, G and Bujdoso, O and Fraunberger, P and Neeser, G and Schmieder, RE and Viell, B and Marget, W and Seewald, M and Walger, P and Stuttmann, R and Speichermann, N and Peckelsen, C and Kurowski, V and Osterhues, HH and Verner, L and Neumann, R and MÃ¼ller-Werdan, U},
