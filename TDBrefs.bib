@Article{Hallal2007,
  Title                    = {Aerosolized tobramycin in the treatment of ventilator-associated pneumonia: a pilot study.},
  Author                   = {Hallal, A and Cohn, SM and Namias, N and Habib, F and Baracco, G and Manning, RJ and Crookes, B and Schulman, CI},
  Journal                  = {Surg Infect (Larchmt)},
