@Article{Stippler2012,
  Title                    = {Brain tissue oxygen monitoring after severe traumatic brain injury in children: relationship to outcome and association with other clinical parameters},
  Author                   = {Stippler, M. and Ortiz, V. and Adelson, P. D. and Chang, Y. F. and Tyler-Kabara, E. C. and Wisniewski, S. R. and Fink, E. L. and Kochanek, P. M. and Brown, S. D. and Bell, M. J.},
