@Article{Seymour2010,
  Title                    = {Out-of-hospital characteristics and care of patients with severe sepsis: A cohort study},
  Author                   = {Seymour, C. W. and Band, R. A. and Cooke, C. R. and Mikkelsen, M. E. and Hylton, J. and Rea, T. D. and Goss, C. H. and Gaieski, D. F.},
  Journal                  = {J Crit Care},
