@Article{Viccellio2001,
  Title                    = {A prospective multicenter study of cervical spine injury in children},
  Author                   = {Viccellio, P. and Simon, H. and Pressman, B. D. and Shah, M. N. and Mower, W. R. and Hoffman, J. R.},
