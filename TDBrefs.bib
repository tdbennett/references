@Article{Farris2013,
  Title                    = {Functional outcomes in pediatric severe sepsis: further analysis of the researching severe sepsis and organ dysfunction in children: a global perspective trial},
  Author                   = {Farris, R. W. and Weiss, N. S. and Zimmerman, J. J.},
