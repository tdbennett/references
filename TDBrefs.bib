@Article{Salonia2012,
  Title                    = {The Utility of Near Infrared Spectroscopy in Detecting Intracranial Hemorrhage in Children},
  Author                   = {Salonia, R. and Bell, M. J. and Kochanek, P. M. and Berger, R. P.},
  Journal                  = {J Neurotrauma},
