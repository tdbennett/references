  Author                   = {Klevens, RM and Morrison, MA and Nadle, J and Petit, S and Gershman, K and Ray, S and Harrison, LH and Lynfield, R and Dumyati, G and Townes, JM and Craig, AS and Zell, ER and Fosheim, GE and McDougal, LK and Carey, RB and Fridkin, SK},
  Journal                  = {JAMA},
